  {
    "name": "Local res",
    "version": "1.1",
    "description": "Local res",
    "permissions": [
      "activeTab",
      "tabs",
      "contextMenus",
      "<all_urls>"
    ],
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    },
    "icons": {
      "128": "icon.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2
  }